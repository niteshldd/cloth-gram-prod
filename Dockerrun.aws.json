{
    "AWSEBDockerrunVersion": 3,
    "containerDefinitions": [
        {
            "name": "web",
            "image": "niteshldd/clothgram:latest",
            "hostname": "web",
            "command": [
                 "python manage.py runserver 0.0.0.0:8000"
                ],
            "essential": true,
            "portMappings": [
              {
                "hostPort": 80,
                "containerPort": 8000
              }
            ],
            "memory": 200
          }
        ]
      }