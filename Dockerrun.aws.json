{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "web",
            "image": "niteshldd/clothgram:latest",
            "hostname": "web",
            "workingDirectory": "/app/",
            "COMMAND": [
                "python3 manage.py runserver 0.0.0.0:8000 && celery -A saleor worker --app=saleor.celeryconf:app --loglevel=info"
                ],
            "essential": true,
            "portMappings": [
              {
                "hostPort": 80,
                "containerPort": 8000
              }
            ],
            "memory": 500
          }
        ]
      }