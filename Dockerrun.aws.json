{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "web",
            "image": "niteshldd/clothgram:latest",
            "hostname": "web",
            "command": ["python manage.py migrate --no-input", "python3 manage.py collectstatic --noinput"],
            "essential": true,
            "portMappings": [
              {
                "hostPort": 80,
                "containerPort": 8000
              }
            ],
            "memory": 200
          }
        ]
      }